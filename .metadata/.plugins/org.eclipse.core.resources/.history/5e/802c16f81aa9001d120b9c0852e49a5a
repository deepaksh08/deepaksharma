package ExcelPrograms;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;

public class ExcelReader1 {
	
	
	public void read(String filelocation, String filename, String sheetname) throws FileNotFoundException {
		
		
		File file = new File(filelocation);
		FileInputStream fileinput = new FileInputStream(file);
		
		
	}

	
	
	public static void main(String[] args) {
		
		String fileLocation = System.getProperty("user.dir") + "\\src\\test\\resources\\" ;
		String filename = "TestExcel.xlsx" ;
		String sheetname = "Sheet1";
		
		ExcelReader1 obj = new ExcelReader1();
		obj.read(fileLocation, filename, sheetname);
		
		System.out.println(fileLocation);
		
	}
}



