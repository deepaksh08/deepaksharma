package ExcelPrograms;

public class ReverseAStringBasic {

	public StringBuilder revstring(String inputstring) {
		StringBuilder outputstring = null;
		
		StringBuilder str = new StringBuilder(inputstring);
		outputstring = str.reverse();
		
		return outputstring;
	}

	public static void main(String[] args) {

		String inputstring = "deepak";

		ReverseAStringBasic Obj = new ReverseAStringBasic();

		System.out.println(Obj.revstring(inputstring));
		

	}

}
